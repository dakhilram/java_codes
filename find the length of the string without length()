// find the length of the string without length();
public class MyClass {
    public static void fs(String a){
        char[] arr=a.toCharArray();
        int c=0;
        for(char i: arr){
            c++;
        }
        System.out.println("count= "+c);
    }
    public static void main(String args[]) {
      String s="abcd";
      fs(s);
      fs("akhil");
    }
}
